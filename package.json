  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "babel-node": "babel-node",
    "start": "npm run babel-node -- ./src/index.js",
    "dev": "cross-env NODE_ENV=dev nodemon --exec babel-node ./src/index.js",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ && cp firebase.config.json dist/",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha test/unit/**/*.test.js",
    "test": "npm run test:unit",
    "lint": "eslint ./src/ --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
